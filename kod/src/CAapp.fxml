<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <HBox layoutX="-1.0" layoutY="-2.0">
               <children>
                  <Pane prefHeight="546.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="14.0" layoutY="14.0" prefHeight="34.0" prefWidth="184.0" text="Model parameters" />
                        <ChoiceBox layoutX="14.0" layoutY="57.0" prefWidth="150.0" />
                        <Label layoutX="14.0" layoutY="120.0" text="Label" />
                        <Button fx:id="initButton" layoutX="42.0" layoutY="507.0" mnemonicParsing="false" onAction="#initClicked" text="INIT" />
                     </children>
                  </Pane>
                  <TabPane prefHeight="546.0" prefWidth="436.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                      <Tab text="Model">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <Canvas fx:id="boardToPaint" height="428.0" layoutX="14.0" layoutY="14.0" width="408.0" />
                                    <Button fx:id="clearButton" layoutX="31.0" layoutY="478.0" mnemonicParsing="false" onAction="#clearClicked" text="Clear" />
                                    <Button fx:id="startButton" layoutX="94.0" layoutY="478.0" mnemonicParsing="false" onAction="#startClicked" text="Start" />
                                    <Button fx:id="stopButton" layoutX="165.0" layoutY="478.0" mnemonicParsing="false" onAction="#stopClicked" text="Stop" />
                                    <Slider fx:id="speedSlider" layoutX="238.0" layoutY="484.0" />
                                    <Label layoutX="262.0" layoutY="461.0" text="Simulation Speed" />
                                 </children>
                              </AnchorPane>
                        </content>
                      </Tab>
                      <Tab text="Charts">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <LineChart fx:id="chart1" layoutX="6.0" layoutY="27.0" prefHeight="400.0" prefWidth="421.0">
                                      <xAxis>
                                        <CategoryAxis side="BOTTOM" />
                                      </xAxis>
                                      <yAxis>
                                        <NumberAxis side="LEFT" />
                                      </yAxis>
                                    </LineChart>
                                 </children></AnchorPane>
                        </content>
                      </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </HBox>
      </children>
    </AnchorPane>
  </children>
</VBox>
